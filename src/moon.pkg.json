    "lookahead_iterator.js.mbt": ["js"],
    "lookahead_iterator.native.mbt": ["native"],
    "node_test.mbt": ["native"],
    "node.js.mbt": ["js"],
    "node.native.mbt": ["native"],
    "parse_test.mbt": ["native"],
    "parser_test.mbt": ["native"],
    "parser.js.mbt": ["js"],
    "parser.native.mbt": ["native"],
